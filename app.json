{
  "expo": {
    "name": "Volleyball",
    "slug": "volleyball-scoreboard",
    "version": "1.0.1",
    "orientation": "default",
    "icon": "./assets/images/main-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/main-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sudhirc.volleyball-scoreboard"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.volleyballscoreboard"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3659763443240462~8135350436",
          "iosAppId": "ca-app-pub-3659763443240462~9690569714"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0819e66a-4228-4ad8-b498-c12bdb8563e5"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/0819e66a-4228-4ad8-b498-c12bdb8563e5"
    }
  }
}
